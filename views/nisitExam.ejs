          <!-- DataTales Example -->
          <div class="card shadow mb-4">
            <div class="card-header py-3 d-sm-flex align-items-center justify-content-between mb-4">
              <h1 class="h3 mb-0 text-gray-800">การสอบ</h1>
            
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr>
                      
                      <th>วิชา</th>
                      <th>กลุ่ม</th>
                      <th>เวลา</th>
                      <th>วันที่</th>
                      <th>ห้อง</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for(var i=0; i < exam.length; i++) { %>
                      <% for(var j=0; j < exam[i].student.length; j++) { %>
                      <%if (exam[i].student[j] == user.user) { %>
                      
                       
                      <tr>
                        <td>
                          <%= exam[i].subjectName%>
                        </td>
                        <td>
                          <%= exam[i].group%>
                        </td>
                        
                        <td>
                          <%= exam[i].timeStart %>-<%= exam[i].timeStop %>
                        </td>
                        <td>
                            <%= exam[i].date %>
                          </td>
                          <td>
                            <%= exam[i].room %>
                          </td>
  
                        
                      </tr>
                      <% } %>
                      <% } %>
                      <% } %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>

        </div>
